@if (ViewBag.AdobeAnalyticsScriptUrl != null && ViewBag.AdobeAnalyticsPageName != null && ViewBag.AdobeAnalyticsPageName != null)
{
    var scripturl = ViewBag.AdobeAnalyticsScriptUrl;
    <script src="@scripturl" type="text/plain" data-cookieconsent="statistics" defer></script>
    <script type="text/javascript">
        window.digitalData = {
            "page": {
                "pageInfo": { "pageName": "@ViewBag.AdobeAnalyticsPageName" },
                "category": @Html.Raw(ViewBag.AdobeAnalyticsCategories)
            }
        };
    </script>
}
